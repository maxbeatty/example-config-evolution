{
  "name": "example-config-evolution",
  "version": "3.0.0",
  "description": "Example of a web app configuration evolving",
  "main": "index.js",
  "scripts": {
    "build": "NOW_TEST=$(node -p \"process.env.hasOwnProperty('NOW_URL')\") && echo $NOW_TEST && confidence -c confidence.json --filter.now=$NOW_TEST > config.json",
    "start": "rejoice -c config.json -p `pwd`",
    "test": "lab -c -L"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxbeatty/example-config-evolution.git"
  },
  "keywords": [
    "example",
    "config",
    "zeit"
  ],
  "author": "Max Beatty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxbeatty/example-config-evolution/issues"
  },
  "homepage": "https://github.com/maxbeatty/example-config-evolution#readme",
  "devDependencies": {
    "code": "3.0.1",
    "eslint": "2.13.1",
    "eslint-config-standard": "5.3.1",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-standard": "1.3.2",
    "lab": "10.8.2"
  },
  "dependencies": {
    "confidence": "3.0.1",
    "good": "7.0.1",
    "good-console": "6.1.2",
    "hapi": "13.5.0",
    "joi": "8.4.2",
    "pg": "6.0.1",
    "rejoice": "3.2.0"
  }
}
